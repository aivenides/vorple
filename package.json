{
  "name": "vorple",
  "version": "3.1.0",
  "description": "Vorple Interactive Fiction User Interface Library",
  "scripts": {
    "compile": "(cd cheapglk && exec emmake make); (cd Git && exec npm run compile)",
    "lint": "jshint src/*.js",
    "start": "webpack-dev-server",
    "build": "webpack",
    "test": "wdio wdio.conf.js",
    "test:chrome": "BROWSER=chrome wdio wdio.conf.js",
    "test:firefox": "BROWSER=firefox wdio wdio.conf.js",
    "test:safari": "BROWSER=safari wdio wdio.conf.js",
    "test:all": "npm run test:chrome && npm run test:firefox && npm run test:safari"
  },
  "repository": "github:vorple/vorple",
  "author": "Juhana Leinonen",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/vorple/vorple/issues"
  },
  "browser": "./dist/interpreter/vorple.min.js",
  "homepage": "https://vorple-if.com",
  "devDependencies": {
    "@wdio/dot-reporter": "^5.11.0",
    "@wdio/local-runner": "^5.11.6",
    "@wdio/mocha-framework": "^5.11.0",
    "@wdio/selenium-standalone-service": "^5.11.2",
    "@wdio/sync": "^5.11.0",
    "chai": "^4.2.0",
    "chai-webdriverio": "^1.0.0",
    "chromedriver": "^75.1.0",
    "copy-webpack-plugin": "^5.0.3",
    "jshint": "^2.10.2",
    "wdio-chromedriver-service": "^5.0.2",
    "webpack": "^4.36.1",
    "webpack-cli": "^3.3.6",
    "webpack-dev-server": "^3.7.2"
  },
  "dependencies": {
    "@wdio/cli": "^5.11.6",
    "jquery": "^3.4.1",
    "jquery-powertip": "^1.3.1",
    "toastr": "^2.1.4",
    "vex-dialog": "^1.1.0",
    "vex-js": "^4.1.0"
  },
  "resolutions": {
    "**/**/fibers": "^4",
    "**/**/fsevents": "^1.2.9"
  }
}
